{
    "extends": "tslint:latest",
    "rules": {
        "ordered-imports": [
            false
        ],
        "variable-name": [
            true,
            "allow-leading-underscore"
        ],
        "whitespace": [
            false
        ],
        "one-line": [
            false
        ],
        "trailing-comma": [false],
        "typedef-whitespace": [
            false
        ],
        "interface-name": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-bitwise": false,
        "quotemark": [
            true, "single", "avoid-escape"
        ],
        "prefer-for-of": false,
        "no-empty-interface": false,
		"indent": [
			true,
			"tabs"
		],
		"no-console": [
			true, "log"
		]
    }
}