// Place your settings in this file to overwrite default and user settings.
{
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true
    },
    "files.trimTrailingWhitespace": false,
    "tslint.configFile": "./tslint.json",
    "tslint.exclude": [
        "./test/**/*.ts",
        "**/*.js"
    ]
}